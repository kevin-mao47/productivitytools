{
    "name": "Productivity Tools",
    "version": "1.0.11",
    "description": "Manage and maximize your productivity!",
    "manifest_version": 2,
    "icons": {"128": "icon_128.png"},
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "permissions":["activeTab"]
}